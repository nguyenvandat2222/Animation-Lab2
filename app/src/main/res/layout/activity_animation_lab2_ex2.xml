        tools:layout_editor_absoluteX="182dp" />

    <ImageView
        android:id="@+id/motam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toTopOf="@+id/shortgreen"
