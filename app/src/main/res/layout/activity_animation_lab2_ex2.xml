        tools:layout_editor_absoluteX="44dp" />

    <ImageView
        android:id="@+id/chauba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toTopOf="@+id/chaubon"
